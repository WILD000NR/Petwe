<!DOCTYPE HTML>
<html>
<head>
	@await Html.PartialAsync("MetatagPatrial")
	@await Html.PartialAsync("CssPatrial")
</head>
<body>
	<div id="page-wrapper">
		@await Html.PartialAsync("HeadderPartial")
		<!-- Main -->
		<div id="main">
			<div class="container">
				<div class="row main-row">
					<div class="col-4 col-12-medium">
						@RenderBody()
					</div>
				</div>
			</div>
		</div>
		<!-- Footer -->
		<div id="footer-wrapper">
			@await Html.PartialAsync("FooterPartial")
		</div>
		@await Html.PartialAsync("SitebarPartial")
	</div>
@await Html.PartialAsync("ScriptsPartial")
</body>
</html>